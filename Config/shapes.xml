        <property name="Model" value="counter_half_hollow_top_offset" />
        <property name="Place" value="TowardsPlacerInverted" />
        <property name="CustomIcon" value="shapeCounterHalfHollowTopOffset" />
        <property name="ShapeCategories" value="Deco" />
        <property name="ImposterExchange" value="imposterBlock" />
        <property name="ShapeAltTexture" value="312,312,312,312,312,312,312" />
        <property name="WaterClipPlane" value="0, 1, -1.192093E-07, -0.5189999" />
        <property name="WaterFlow" value="permitted" />
        <property name="ModelOffset" value="1,.01,.06"/>
    </shape>
	
